{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "tau: run",
            "type": "cargo",
            "command": "run",
            "args": [
                "--package=tau-init",
                "--target=aarch64-unknown-none-softfloat",
                "-Z build-std=core",
                "--release"
            ],
            "problemMatcher": [
                "$rustc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}